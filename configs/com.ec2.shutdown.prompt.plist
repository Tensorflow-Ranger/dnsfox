<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.ec2.shutdown.prompt</string>

  <key>LimitLoadToSessionType</key>
  <string>Aqua</string>

  <key>ProgramArguments</key>
  <array>
    <string>/Applications/EC2ShutdownPrompt.app/Contents/MacOS/applet</string>
  </array>

  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>22</integer>
    <key>Minute</key>
    <integer>45</integer>
  </dict>

  <key>RunAtLoad</key>
  <false/>
</dict>
</plist>
